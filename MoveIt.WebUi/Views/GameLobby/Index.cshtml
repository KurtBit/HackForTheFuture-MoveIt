@model IEnumerable<MoveIt.WebUi.ViewModels.TeamViewModel>

@{
    ViewBag.Title = "Game Lobby";
}

<section class="game-lobby">
    <div class="container-fluid">
        <div class="row">
            <div class="col-lg-4"></div>
            <div class="col-lg-4">
                <h2 class="text-center">Game Lobby</h2>
                <div class="row">
                    <div class="col-lg-12">
                        Location Displayed here:
                    </div>
                </div>
                <div class="row">
                    <div class="col-lg-offset-2 col-lg-4">
                        <button type="submit">Agree/Play</button>
                    </div>
                    <div class="col-lg-4">
                        <button type="reset">I don't think so</button>
                    </div>
                </div>
            </div>
            <div class="col-lg-4"></div>
        </div>
    </div>
</section>