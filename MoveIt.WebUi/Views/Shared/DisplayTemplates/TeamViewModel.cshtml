@model MoveIt.WebUi.ViewModels.TeamViewModel

<div class="col-lg-offset-3 col-lg-6">
    @foreach (var user in Model.Users)
    {
        <div class="row">
            <div class="col-lg-offset-3 col-lg-6">
                <div class="team-user-photo">
                    <a href="#" class="thumbnail">
                        <img src="@user.AvatarImgUrl" alt="Member Image" />
                    </a>
                </div>
            </div>
            <div class="col-lg-12">
                <div class="team-user-name text-center">
                    @user.UserName
                </div>
            </div>
        </div>
    }
</div>
